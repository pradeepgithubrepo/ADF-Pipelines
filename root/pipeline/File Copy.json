{
	"name": "File Copy",
	"properties": {
		"activities": [
			{
				"name": "6063_Copydata",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "BinarySource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"wildcardFileName": "*.png"
						}
					},
					"sink": {
						"type": "BinarySink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings",
							"copyBehavior": "PreserveHierarchy"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "6063_src_ds",
						"type": "DatasetReference",
						"parameters": {
							"filename": "@pipeline().parameters.sourcefile"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "6063_dst_ds",
						"type": "DatasetReference"
					}
				]
			}
		],
		"parameters": {
			"sourcefile": {
				"type": "string"
			}
		},
		"annotations": []
	}
}
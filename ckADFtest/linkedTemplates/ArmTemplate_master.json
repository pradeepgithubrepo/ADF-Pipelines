{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ckADFtest"
		},
		"comcast_ADLS_LS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'comcast_ADLS_LS'"
		},
		"AzureBlobStorage1_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "pradstorage"
		},
		"comcast_ADLS_LS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://hcscadls.dfs.core.windows.net"
		},
		"prad_keyvault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://hcscpockeyvault2.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ckADFtest_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"comcast_ADLS_LS_accountKey": {
						"value": "[parameters('comcast_ADLS_LS_accountKey')]"
					},
					"comcast_ADLS_LS_properties_typeProperties_url": {
						"value": "[parameters('comcast_ADLS_LS_properties_typeProperties_url')]"
					},
					"prad_keyvault_properties_typeProperties_baseUrl": {
						"value": "[parameters('prad_keyvault_properties_typeProperties_baseUrl')]"
					},
					"AzureBlobStorage1_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('AzureBlobStorage1_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}